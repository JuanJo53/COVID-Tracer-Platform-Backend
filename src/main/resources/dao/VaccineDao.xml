<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.covid_tracer_platform_backend.dao.VaccineDao">
    <insert id="createVaccine" parameterType="bo.ucb.edu.covid_tracer_platform_backend.model.Vaccine">
        INSERT INTO vaccine VALUES (
            null, #{vaccineTypeId}, #{countryId}, #{departmentId}, #{vaccinated}, #{cumulativeVaccinated}, #{date},
            1, #{transaction.txId}, #{transaction.txHost}, #{transaction.txUserId}, #{transaction.txDate}
        )
    </insert>
</mapper>
